<nav>
  {%- for nav in collections.nav | reverse %}
        <a
          {% if nav.url == page.url %}
            class="button button-primary pointer-events-none"
          {% else %}
            class="button"
          {% endif %}
          href='{{ nav.url | url }}'
          >
        {{ nav.data.navtitle }}
        </a>
  {%- endfor -%}
</nav>
