<nav>
  {%- for nav in collections.nav | reverse %}
        <button 
          {% if nav.url == page.url %}
          class="button-primary"
          disabled=true
          {% endif %}
          {% if nav.url != page.url %}onclick="window.location.href='{{ nav.url | url }}'"{% endif %}
          >
        {{ nav.data.navtitle }}
        </button>
  {%- endfor -%}
</nav>
